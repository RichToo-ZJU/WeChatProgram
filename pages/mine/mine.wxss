/**index.wxss**/
.page_bg{
  background-color: white;
  height: 1350rpx;
}

.title{
  font-size: 40rpx;
  font-weight: normal;
  text-align: center;
  color: #ffffff;
  background-color: #0eafaf;
  margin-top: 50px;
}

.title_box{
  flex-direction:row;
  width: 100%;
  height: 350rpx;
  background: #0eafaf ;
  border-radius: 50rpx;
  margin-top:-50rpx;
}

.float_left{
  float:left;
}

.user_info_bg{
  flex-direction:row;
  width: 92%;
  height: 160rpx;
  box-shadow: 0rpx 8rpx 40rpx rgb(111, 133, 120); 
  background: #ffffffd7;
  margin: -90rpx 30rpx 80rpx 30rpx; 
  border-radius: 18rpx;
}

.user_img{
  text-align: left;
  height: 120rpx;
  width: 120rpx;
  margin-top: 18rpx;
  margin-left: 10rpx;
  margin-bottom: 5rpx;
  border-radius: 20rpx;
}

.txt{
  display: flex;
  flex-flow:column; 
  flex-wrap: wrap;
}

.user_name{
  white-space: nowrap;
  line-height: 10px;
  color: rgb(46, 52, 75);
  font-size: 40rpx;
  font-weight: 600;
  line-height: 41px;
  margin-left:20rpx ;
  margin-top: 10rpx;
}

.user_number{
  position:relative;
  white-space: nowrap;
  line-height: 10px;
  color: rgb(176, 179, 199);
  margin-top:10rpx ;
  margin-left:20rpx ;
  font-size:30rpx;
}

.content_bg{
  box-shadow: 0rpx 8rpx 40rpx rgb(111, 133, 120); 
  width: 92%;
  height: 728rpx;
  border-radius: 18rpx;
  margin-left: 26rpx;
}
@import "/pages/tabbar/tabbar.wxss";
